{
    "compilerOptions": {
      "module": "CommonJS",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "outDir": "../out",
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./**/*"],
    "exclude":  ["./**/*.spec"]
  }
  