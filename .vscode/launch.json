    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "out/index.js",
            "outFiles": [
                "${workspaceFolder}/out/*.js"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        },
        {
            "name": "vectorize",
            "type": "python",
            "request": "launch",
            "program": "search-engine/vectorizer.py",
            "args": ["--missing"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        },
        {
            "name": "vectorize update",
            "type": "python",
            "request": "launch",
            "program": "search-engine/vectorizer.py",
            "args": ["dPljx0DGKcX0FuPmvpRB.png"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        },        
        {
            "name": "search",
            "type": "python",
            "request": "launch",
            "program": "vectorizer.py",
            "args": ["serve"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        }
    ]
}