{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "pycollector",
            "args": ["update-remote-index"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "out/index.js",
            "outFiles": [
                "${workspaceFolder}/out/*.js"
            ],
            "preLaunchTask": "npm: build",
            "cwd": "${workspaceFolder}",
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1",
                // "USE_PYCOLLECTOR": "1"
            }
        },
        {
            "name": "vectorize",
            "type": "debugpy",
            "request": "launch",
            "program": "search-engine/vectorizer.py",
            "args": [
                "--missing"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        },
        {
            "name": "vectorize update",
            "type": "debugpy",
            "request": "launch",
            "program": "search-engine/vectorizer.py",
            "args": [
                "dPljx0DGKcX0FuPmvpRB.png"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        },
        {
            "name": "search",
            "type": "debugpy",
            "request": "launch",
            "program": "vectorizer.py",
            "args": [
                "serve"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1"
            }
        }
    ]
}