{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "pycollector",
            "args": ["cluster"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "out/index.js",
            "outFiles": [
                "${workspaceFolder}/out/*.js"
            ],
            "preLaunchTask": "npm: build",
            "cwd": "${workspaceFolder}",
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1",
                // "USE_PYCOLLECTOR": "1"
            }
        },
        {
            "name": "Run update dup index",
            "program": "${workspaceFolder}/out/scripts/updateDupIndex.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "preLaunchTask": "npm: build",
            "env": {
                "PROJECT_ID": "macarons-410004",
                "REGION": "northamerica-northeast1",
                // "USE_PYCOLLECTOR": "1"
            }
        }
    ]
}